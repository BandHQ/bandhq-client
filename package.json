{
  "name": "bandhq-client",
  "version": "1.0.3",
  "private": true,
  "description": "Contains the front end of the BandHQ App",
  "homepage": "/",
  "dependencies": {
    "@apollo/react-hooks": "3.0.0",
    "@date-io/dayjs": "^1.3.13",
    "@reach/router": "^1.2.1",
    "@sentry/browser": "^5.11.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "apollo": "^2.21.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-retry": "^2.2.15",
    "apollo-upload-client": "^12.1.0",
    "clsx": "^1.0.4",
    "dayjs": "^1.8.19",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "formik": "^1.5.8",
    "formik-error-focus": "^1.0.0",
    "graphql-tag": "^2.10.1",
    "notistack": "^0.9.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-mailchimp-subscribe": "^2.1.0",
    "react-scripts": "^3.3.0",
    "react-spinners": "^0.8.0",
    "react-spring": "^8.0.27",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.6",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "babel-loader": "^8.0.6",
    "customize-cra": "^0.6.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-import-resolver-babel-module": "^5.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "react-app-rewired": "^2.1.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "react-app",
      "airbnb",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "max-len": [
        "warn",
        {
          "code": 120,
          "ignoreStrings": true
        }
      ],
      "import/no-unresolved": [
        "error",
        {
          "caseSensitive": false
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "jsx-a11y/label-has-for": [
        2,
        {
          "components": [
            "Label"
          ],
          "required": {
            "every": [
              "id"
            ]
          }
        }
      ],
      "jsx-a11y/anchor-has-content": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "import/prefer-default-export": "off"
    }
  },
  "scripts": {
    "start": "node server.js",
    "dev": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BandHQ/bandhq-client.git"
  },
  "author": "Tom Brierlewy",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/BandHQ/bandhq-client/issues"
  },
  "engines": {
    "node": "12.x"
  }
}
